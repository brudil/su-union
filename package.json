  "scripts": {
    "storybook": "start-storybook -p 9001 -c ./config/storybook",
    "prod": "export NODE_ENV=production && export BABEL_ENV=bundle && `yarn bin`/webpack --config ./config/webpack.prod.config.js",
    "falmer:prod": "export NODE_ENV=production && export BABEL_ENV=bundle && `yarn bin`/webpack --config ./config/falmer.prod.config.js",
    "gen": "BABEL_ENV=comp `yarn bin`/comp",
    "deploy": "uncommitted && yarn run prod && s3cmd put --recursive --add-header=\"Cache-Control: public, max-age=365000000, immutable\" ./dist/ s3://ussu-static/ --config=.s3cfg --acl-public && yarn run gen",
    "serve": "node --trace-warnings ./devserver.js",
    "format": "`yarn bin`/prettier \"src/**/*.{ts,tsx,js,css,graphql}\" --write",
    "test": "NODE_ENV=test && jest",
    "testc": "NODE_ENV=test && jest --coverage",
    "falmer:serve": "webpack-dev-server --config ./config/falmer.dev.config.js",
    "falmer:_deploy": "node ./config/deployToFalmer.js",
    "falmer:deploy": "yarn run falmer:prod && s3cmd put --recursive --add-header=\"Cache-Control: public, max-age=365000000, immutable\" ./dist-falmer/ s3://ussu-static/ --config=.s3cfg --acl-public && yarn run falmer:_deploy"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn format"
    }
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "~libs/(.*)": "<rootDir>/src/libs/$1",
      "~falmer/(.*)": "<rootDir>/src/projects/falmer/$1",
      "~components/(.*)": "<rootDir>/src/components/$1"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/(build|docs|node_modules)/",
      "/src/projects/app/"
    ],
    "testEnvironment": "jsdom",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.{svg,d.ts,stories.tsx}"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.jsx?$": "babel-jest"
    },
    "setupFiles": [
      "<rootDir>/__mocks__/browserMocks.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/config/setupTests.js",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "useBabelrc": true
      }
    }
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "always"
  }
}
