  "scripts": {
    "prod": "export NODE_ENV=production && `npm bin`/webpack --config webpack.prod.config.js",
    "gen": "node ./generator/start.js",
    "deploy": "uncommitted && yarn run prod && s3cmd put --recursive --add-header=\"Cache-Control: public, max-age=365000000, immutable\" ./dist/ s3://ussu-static/ --config=.s3cfg --acl-public && yarn run gen",
    "precommit": "`npm bin`/eslint",
    "serve": "BABEL_ENV=dev-server node ./generator/start-server.js",
    "docs:serve": "export NODE_ENV=development && node ./docs/devserver.js",
    "docs:prod": "export NODE_ENV=production && webpack --config ./docs/webpack.prod.config.js"
  },
  "jest": {
    "moduleFileExtensions": [
      "jsx",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/jest/FileStub.js",
      "^.+\\.css$": "<rootDir>/config/jest/CSSStub.js"
    },
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(build|docs|node_modules)/"
    ],
    "testEnvironment": "node"
  }
