  "scripts": {
    "prod": "export NODE_ENV=production && `yarn bin`/webpack --config webpack.prod.config.js",
    "gen": "`yarn bin`/comp",
    "deploy": "uncommitted && yarn run prod && s3cmd put --recursive --add-header=\"Cache-Control: public, max-age=365000000, immutable\" ./dist/ s3://ussu-static/ --config=.s3cfg --acl-public && yarn run gen",
    "precommit": "`npm bin`/eslint",
    "serve": "BABEL_ENV=dev-server node ./devserver.js",
    "docs:serve": "export NODE_ENV=development && node ./docs/devserver.js",
    "docs:prod": "export NODE_ENV=production && webpack --config ./docs/webpack.prod.config.js",
    "format": "prettier --single-quote --print-width 80 --trailing-comma es5 --write \"./{src,generator}/**/*.js\"",
    "test": "NODE_ENV=test && jest",
    "testc": "NODE_ENV=test && jest --coverage"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/(build|docs|node_modules)/"
    ],
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "{generator,src}/**/*.{js,jsx}"
    ]
  }
