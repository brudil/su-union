{
  "name": "@ussu/falmer",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@emotion/core": "^10.0.10",
    "date-fns": "^2.0.0-alpha.31",
    "immer": "^3.1.3",
    "react": "^16.8.6",
    "react-apollo-hooks": "^0.4.5",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-jsonschema-form": "^1.6.1",
    "react-select": "^3.0.4",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.3"
  },
  "scripts": {
    "falmer:prod": "export NODE_ENV=production && export BABEL_ENV=bundle && `yarn bin`/webpack --config ./falmer.prod.config.js",
    "falmer:serve": "webpack-dev-server --config ./falmer.dev.config.js",
    "falmer:_deploy": "node ./config/deployToFalmer.js",
    "falmer:deploy": "yarn run falmer:prod && s3cmd put --recursive --add-header=\"Cache-Control: public, max-age=365000000, immutable\" ./dist-falmer/ s3://ussu-static/ --config=.s3cfg --acl-public && yarn run falmer:_deploy"
  },
  "devDependencies": {
    "@types/react-jsonschema-form": "^1.3.2",
    "@types/react-select": "^2.0.19",
    "html-webpack-harddisk-plugin": "^1.0.1",
    "html-webpack-plugin": "^3.2.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.1"
  }
}
