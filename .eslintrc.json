{
  "extends": [
    "airbnb",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "prettier"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "allowImportExportEverywhere": true,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "jest": true
  },
  "rules": {
    "prettier/prettier": ["error", { "singleQuote": true, "bracketSpacing": true, "trailingComma": "es5" }],
    "no-console": 0,
    "react/jsx-no-bind": 0,
    "import/no-extraneous-dependencies": 0,
    "max-len": [1, 120],
    "no-underscore-dangle": [0],
    "jsx-a11y/no-static-element-interactions": [0],
    "jsx-a11y/no-autofocus": [0],
    "jsx-a11y/no-noninteractive-element-interactions": [0],
    "react/no-array-index-key": [0],
    "react/prop-types": [0],
    "react/jsx-filename-extension": [1, { "extensions": [".js"] }],
    "import/extensions": ["error", { "js": "never", "mjs": "never" }],
    "import/no-unresolved": [0]
  }
}
