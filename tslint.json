{
  "extends": [
    "tslint-config-airbnb",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rules": {
    "variable-name": [true, "ban-keywords", "check-format", "allow-pascal-case", "allow-leading-underscore"],
    "import-name": [false],
    "jsx-boolean-value": ["never"],
    "function-name": [false],
    "jsx-no-lambda": [false]
  }
}
